{
  "name": "gasoline-mcp",
  "version": "6.1.7",
  "description": "Gasoline MCP is an AI Debugger, Inspector, and Observability toolkit. It acts as Chrome DevTools for AI Agents using the Model Context Protocol (MCP).",
  "license": "AGPL-3.0-only",
  "bin": {
    "gasoline-mcp": "bin/gasoline-mcp"
  },
  "scripts": {
    "preinstall": "node lib/kill-daemon.js",
    "postinstall": "node lib/postinstall-skills.js",
    "preuninstall": "node lib/kill-daemon.js"
  },
  "files": [
    "bin",
    "lib",
    "skills"
  ],
  "optionalDependencies": {
    "@brennhill/gasoline-darwin-arm64": "6.1.7",
    "@brennhill/gasoline-darwin-x64": "6.1.7",
    "@brennhill/gasoline-linux-arm64": "6.1.7",
    "@brennhill/gasoline-linux-x64": "6.1.7",
    "@brennhill/gasoline-win32-x64": "6.1.7"
  },
  "keywords": [
    "mcp",
    "mcp-server",
    "ai-debugging",
    "ai-observability",
    "llm-ops",
    "devtools",
    "prompt-engineering",
    "agent-inspection"
  ],
  "homepage": "https://cookwithgasoline.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/brennhill/gasoline-mcp-ai-devtools"
  },
  "bugs": {
    "url": "https://github.com/brennhill/gasoline-mcp-ai-devtools/issues"
