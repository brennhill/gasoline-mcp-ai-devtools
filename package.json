{
  "name": "gasoline-mcp",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test:ext": "node --test --test-force-exit --test-concurrency=8 tests/extension/*.test.js extension/background/*.test.js",
    "test:go": "make test",
    "test": "npm run test:go && npm run test:ext",
    "check": "npm run lint && npm run typecheck && npm run format"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/chrome": "^0.1.36",
    "esbuild": "^0.27.2",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-security": "^3.0.0",
    "globals": "^17.3.0",
    "prettier": "^3.3.0",
    "typescript": "^5.5.0"
  },
  "homepage": "https://cookwithgasoline.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/brennhill/gasoline-mcp-ai-devtools"
  },
  "bugs": {
    "url": "https://github.com/brennhill/gasoline-mcp-ai-devtools/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
